labels = c("Male", "Female"), ordered = FALSE)
df8         <- data.frame(Consumption, Income, Gender)
df8
fm8 <- lm(formula = Consumption ~ Income + Gender, data = df8)
summary(fm8)
##----ancova.fm8anova----
anova(fm8)
##----ancova.p8----
library(emmeans)
##----ancova.fm8anova----
anova(fm8)
emmip(fm8, Gender ~ Income)
library(ggplot2)
p8 <-
